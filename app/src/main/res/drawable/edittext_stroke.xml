<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <corners android:radius="15dp" /> <!-- Ubah radius sesuai keinginan -->
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="1dp"
            android:color="@color/colorStroke" /> <!-- Ganti dengan warna stroke yang diinginkan -->
            <corners android:radius="15dp" /> <!-- Ubah radius sesuai keinginan -->
        </shape>
    </item>
</layer-list>
